<!DOCTYPE html>
<html>
  <head>
    <title>EDA Phase 1 Express Boilerplate</title>
  </head>
  <body>
    <h1>People of EDA Land</h1>
    <ul>
    {{#each users}}
      <li><a href="/profile/{{id}}">{{name}}</a></li>
    {{/each}}
    </ul>
    <form action="/users" method="post">
       Profile Name: <input type="text" name="name">
      <br>
       Profile Picture: <input type="text" name="image">
      <br>
       Email: <input type="text" name="email">
      <br>
       Website: <input type="text" name="url">
       <br>
       <input type="submit" value="Submit">
    </form>
  </body>
</html>
